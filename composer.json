{
    "name": "offloc/router",
    "type": "library",
    "description": "Offloc router",
    "homepage": "https://github.com/offloc/offloc-router",
    "keywords": ["url", "shortener"],
    "license": "MIT",
    "authors": [
        {
            "name": "Offloc Incorporated",
            "email": "info@offloc.com",
            "homepage": "http://offloc.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "doctrine/common": ">=2.2.3,<2.4-dev"
    },
    "require-dev": {
        "dflydev/identity-generator": "1.*",
        "doctrine/orm": ">=2.2.3,<2.4-dev",
        "lootils/uuid": "1.*@dev"
    },
    "replace": {
        "offloc/router-api-client": "self.version",
        "offloc/router-api-common": "self.version"
    },
    "suggest": {
        "dflydev/base32-crockford": "Required for base32 crockford based dflydev/identity-generator generator",
        "dflydev/identity-generator": "Required to use dflydev/identity-generator based identity generators",
        "dflydev/identity-generator-dbal": "Solid candidate for dflydev/identity-generator data store implementation",
        "doctrine/orm": "Required for Doctrine based persistence",
        "lootils/uuid": "Required for UUID secret generator"
    },
    "autoload": {
        "psr-0": {
            "Offloc\\Router": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
