{
    "name": "offloc/prism",
    "type": "library",
    "description": "URL Shortener + Custom Header Injector",
    "homepage": "https://github.com/offloc/offloc-prism",
    "keywords": ["url", "shortener"],
    "license": "MIT",
    "authors": [
        {
            "name": "Offloc Incorporated",
            "email": "info@offloc.com",
            "homepage": "http://offloc.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "doctrine/common": ">=2.2.3,<2.4-dev"
    },
    "require-dev": {
        "dflydev/identity-generator": "1.*",
        "doctrine/orm": "~2.3",
        "lootils/uuid": "1.*@dev"
    },
    "replace": {
        "offloc/prism-api-client": "self.version",
        "offloc/prism-api-common": "self.version",
        "offloc/prism-domain": "self.version",
        "offloc/prism-infrastructure-persistence-doctrine": "self.version"
    },
    "suggest": {
        "doctrine/orm": "~2.3",
        "lootils/uuid": "1.*"
    },
    "autoload": {
        "psr-0": {
            "Offloc\\Prism": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
